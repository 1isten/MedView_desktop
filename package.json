{
  "author": "STEN <sten.li@foxmail.com>",
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "start": "npm-run-all2 -pr dev:volview dev forge",
    "preforge": "npm-run-all2 -s generate:proto generate:electron",
    "forge": "electron-forge start",
    "prepackage": "npm-run-all2 -s build:volview generate:proto generate:electron generate",
    "package": "electron-forge package",
    "premake": "npm-run-all2 -s build:volview generate:proto generate:electron generate",
    "make": "electron-forge make",
    "dev": "nuxt dev",
    "dev:volview": "cd VolView && npm run dev",
    "build": "nuxt build",
    "build:volview": "cd VolView && npm ci && npm run build && cd .. && shx rm -rf public/volview && shx mv VolView/dist public/volview",
    "generate": "nuxt generate",
    "generate:proto": "buf generate",
    "generate:electron": "esbuild electron/*.ts --format=cjs --platform=node --minify --bundle --packages=external --outdir=electron --allow-overwrite",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@connectrpc/connect": "^2.1.1",
    "@connectrpc/connect-node": "^2.1.1",
    "@connectrpc/connect-web": "^2.1.1",
    "@connectrpc/validate": "^0.2.0",
    "@msgpack/msgpack": "^3.1.3",
    "@trpc/client": "^11.9.0",
    "@trpc/server": "^11.9.0",
    "dcmjs": "^0.49.1",
    "dcmjs-imaging": "^0.2.2",
    "h3": "^1.15.5"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.64.0",
    "@bufbuild/protobuf": "^2.11.0",
    "@bufbuild/protoc-gen-es": "^2.11.0",
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@iconify-json/lucide": "^1.2.87",
    "@iconify-json/mdi": "^1.2.3",
    "@mdi/font": "^7.4.47",
    "@nuxt/ui": "^4.4.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@pinia/nuxt": "^0.11.3",
    "@vueuse/components": "^14.2.0",
    "@vueuse/core": "^14.2.0",
    "@vueuse/nuxt": "^14.2.0",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "npm-run-all2": "^8.0.4",
    "nuxt": "4.2.2",
    "overlayscrollbars-vue": "^0.5.9",
    "shx": "^0.4.0",
    "tailwindcss": "^4.1.18",
    "vuetify": "^3.11.8",
    "vuetify-nuxt-module": "^0.19.5"
  },
  "overrides": {
    "vue": "latest"
  },
  "license": "MIT"
}
